	<assert>
		<scope>BR-E-01</scope>
	</assert>
	<test>
		<assert>
			<description>Verify that an Invoice that contains a line, a document level allowance or
				charge where the invoiced item VAT category code (BT-142) is “E” shall contain in
				the VAT breakdown (BG-21) one VAT category code (BT-111) equal with
				"E".</description>
			<success>BR-E-01</success>
		</assert>
		<Invoice
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
			<cac:TaxTotal>
				<cac:TaxSubtotal>
					<cac:TaxCategory>
						<cbc:ID>E</cbc:ID>
					</cac:TaxCategory>
				</cac:TaxSubtotal>
			</cac:TaxTotal>
			
			<cac:AllowanceCharge>
				<cbc:ChargeIndicator>false</cbc:ChargeIndicator>
				<cac:TaxCategory>
					<cbc:ID>E</cbc:ID>
				</cac:TaxCategory>
			</cac:AllowanceCharge>
			
			<cac:AllowanceCharge>
				<cbc:ChargeIndicator>true</cbc:ChargeIndicator>
				<cac:TaxCategory>
					<cbc:ID>E</cbc:ID>
				</cac:TaxCategory>
			</cac:AllowanceCharge>
			
			<cac:InvoiceLine>
				<cac:Item>
					<cac:ClassifiedTaxCategory>
						<cbc:ID>E</cbc:ID>
					</cac:ClassifiedTaxCategory>
				</cac:Item>
			</cac:InvoiceLine>
		</Invoice>
	</test>
