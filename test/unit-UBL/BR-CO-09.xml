<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0" configuration="tc434-ubl">
	<assert>
		<scope>BR-CO-09</scope>
	</assert>
	<test>
		<assert>
			<description>Verify that the Seller VAT identifier, Seller tax representative VAT
				identifier, Buyer VAT identifier have a prefix in accordance with ISO code ISO
				3166-1 alpha-2</description>
			<success>BR-CO-09</success>
		</assert>
		<Invoice
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
			<cac:AccountingSupplierParty>
				<cac:Party>
					<cac:PartyTaxScheme>
						<cbc:CompanyID>IT123456789</cbc:CompanyID>
					</cac:PartyTaxScheme>
				</cac:Party>
			</cac:AccountingSupplierParty>

			<cac:AccountingCustomerParty>
				<cac:Party>
					<cac:PartyTaxScheme>
						<cbc:CompanyID>EL987654321</cbc:CompanyID>
					</cac:PartyTaxScheme>
				</cac:Party>
			</cac:AccountingCustomerParty>

			<cac:TaxRepresentativeParty>
				<cac:PartyTaxScheme>
					<cbc:CompanyID schemeID="NL:VAT">NL-967611265</cbc:CompanyID>
				</cac:PartyTaxScheme>
			</cac:TaxRepresentativeParty>

		</Invoice>
	</test>

	<test>
		<assert>
			<description>Trigger error if Seller VAT identifier does not have a prefix in accordance with ISO code ISO
				3166-1 alpha-2</description>
			<error>BR-CO-09</error>
		</assert>
		<Invoice
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">

			<cac:AccountingSupplierParty>
				<cac:Party>
					<cac:PartyTaxScheme>
						<cbc:CompanyID>A123456789</cbc:CompanyID>
					</cac:PartyTaxScheme>
				</cac:Party>
			</cac:AccountingSupplierParty>
			

		</Invoice>
	</test>
	<test>
		<assert>
			<description>Trigger error if Buyer VAT identifier does not have a prefix in accordance with ISO code ISO
				3166-1 alpha-2</description>
			<error>BR-CO-09</error>
		</assert>
		<Invoice
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">

			
			<cac:AccountingCustomerParty>
				<cac:Party>
					<cac:PartyTaxScheme>
						<cbc:CompanyID>987654321</cbc:CompanyID>
					</cac:PartyTaxScheme>
				</cac:Party>
			</cac:AccountingCustomerParty>

		</Invoice>
	</test>
	<test>
		<assert>
			<description>Trigger error if Seller tax representative VAT
				identifier does not have a prefix in accordance with ISO code ISO
				3166-1 alpha-2</description>
			<error>BR-CO-09</error>
		</assert>
		<Invoice
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2">
			
			
			<cac:TaxRepresentativeParty>
				<cac:PartyTaxScheme>
					<cbc:CompanyID>967611265</cbc:CompanyID>
				</cac:PartyTaxScheme>
			</cac:TaxRepresentativeParty>
		</Invoice>
	</test>

</testSet>
