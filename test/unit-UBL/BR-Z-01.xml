				<cac:Item>
					<cac:ClassifiedTaxCategory>
						<cbc:ID>Z</cbc:ID>
					</cac:ClassifiedTaxCategory>
				</cac:Item>
			</cac:InvoiceLine>
		</Invoice>
	</test>
